Host *
  IdentitiesOnly yes

Host localhost
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  UserKnownHostsFile /dev/null

Host charon
  Hostname localhost
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan -h @xchan_8286 -d charon -u chant -x 2>/dev/null) -W "%h:%p" -o "StrictHostKeyChecking=no"

Host hades
  Hostname localhost
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan -h @xchan_8286 -d hades -u chant -x 2>/dev/null) -W "%h:%p" -o "StrictHostKeyChecking=no"

Host uplink
  Hostname localhost
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan_8286 -h @xchan_8286 -d uplink -u chant -x 2>/dev/null) -W "%h:%p" -o "StrictHostKeyChecking=no"

