host 192.168.8.1
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedAlgorithms +ssh-rsa

Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519

Host charon
  Hostname localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan -h @xchan_8286 -d charon -u chant -x -o "-W %h:%p,-o StrictHostKeyChecking=no" 2>/dev/null)

Host hades
  Hostname localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan -h @xchan_8286 -d hades -u chant -x -o "-W %h:%p,-o StrictHostKeyChecking=no" 2>/dev/null)

Host uplink
  Hostname localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ProxyCommand=$(sshnp -f @xavierchanth -t @xchan_8286 -h @xchan_8286 -d uplink -u chant -x -o "-W %h:%p,-o StrictHostKeyChecking=no" 2>/dev/null)

