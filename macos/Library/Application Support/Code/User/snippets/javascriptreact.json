{
	// Place your snippets for javascriptreact here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print Var to console": {
    "prefix": "debug",
    "body": ["console.log(\"DEBUG $1:\", typeof($1), \"=>\", $1)$2"],
    "description": "Log variables to console"
  },
  "Print to console": {
    "prefix": "log",
    "body": ["console.log($1)$2"],
    "description": "Log output to console"
  },
  "React Hook": {
    "prefix": "hook",
    "body": [
      "const $1 = (props) => {",
      "$0",
      "}",
      "",
      "export default $1"
    ],
    "description": "Create react component hook."
  },
  "GraphQL Resolver": {
    "prefix": "resolver",
    "body": ["$1(_parent, args, context, info) {", "\t$0", "},"],
    "description": "Create Graphql resolver."
  }
}
