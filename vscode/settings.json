    "/Users/chant/.vscode/extensions/continue.continue-0.6.8-darwin-arm64",
    "/Users/chant/.vscode/extensions/continue.continue-0.6.9-darwin-arm64",
    "/Users/chant/.vscode/extensions/continue.continue-0.6.10-darwin-arm64",
    "/Users/chant/.vscode/extensions/continue.continue-0.6.13-darwin-arm64",
